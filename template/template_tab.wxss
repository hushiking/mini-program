.baricon {
  width: 40rpx;
  height: 36rpx;
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  top: 18rpx;
}

.tabBar {
  width: 100%;
  height: 98rpx;
  position: fixed;
  display: flex;
  bottom: 0;
  background: #fff;
  font-size: 20rpx;
  font-weight: 700;
  color: #8A8A8A;
  border-top: 2rpx solid #cfcfcf;
}

.tabBar-item {
  position: relative;
  flex: 1;
  text-align: center;
  overflow: hidden;
}

.bartext {
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  bottom: 14rpx;
}

.tabBartext {
  color: rgb(40, 182, 42);
}

.navigator-hover {
  background-color: #fff;
  opacity: 1;
}
